<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id= "com.seakerInnovations.golfLeaguer"
		version = "1.0.0">
    <name>GolfLeagueOne</name>
    <description>
        The finest way to track and report on your golf league in real time.
    </description>
    <author email="yarbrough40@yahoo.com" href="http://seekerportal.com">
        Seeker Innovations, LLC
    </author>
    <content src="index.html" />
	<gap:splash src="splash.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
